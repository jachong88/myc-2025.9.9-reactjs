# MYC Studio - Production Environment Configuration
# This file is used when NODE_ENV=production (npm run build)

# API Configuration - Production Server
# Update these URLs to point to your production backend
VITE_API_BASE_URL=https://api.yourdomain.com/api
VITE_API_TIMEOUT=30000

# Environment
VITE_ENV=production

# Feature Flags - Disable MSW and debugging in production
VITE_USE_MOCK_API=false
VITE_USE_MSW=false

# Firebase Configuration (Production)
# Use your production Firebase project settings
REACT_APP_FIREBASE_API_KEY=your-prod-firebase-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-prod-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-prod-firebase-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-prod-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789
REACT_APP_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# Debug Configuration (disabled for production)
VITE_DEBUG_API=false
VITE_DEBUG_AUTH=false

# Development Server Configuration (for preview mode)
VITE_DEV_SERVER_PORT=5173
VITE_DEV_SERVER_HOST=localhost
VITE_AUTO_OPEN_BROWSER=false
